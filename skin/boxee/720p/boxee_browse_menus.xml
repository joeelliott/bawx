<?xml version="1.0"?>
<includes>
	<include name="BrowseLeftMenu">
		<animation effect="fade" start="100" end="25" time="200" condition="Window.IsActive(boxee_dropdown.xml)">Conditional</animation>			
		<posx>22</posx>
		<posy>16</posy>
		<width>270</width>
		<height>720</height>
		<itemgap>12</itemgap>
		<orientation>vertical</orientation>
		<usecontrolcoords>false</usecontrolcoords>
		<onup>1000</onup>
		<ondown>-</ondown>
		<onleft>-</onleft>
		<onright>9000</onright>
	</include>
	<include name="BrowseLeftMenuBackground">
		<animation effect="fade" start="100" end="25" time="200" condition="Window.IsActive(DialogProgress.xml)">Conditional</animation>
		<posx>0</posx>
		<posy>0</posy>
		<width>270</width>
		<height>720</height>
		<control type="image">
			<posx>0</posx>
			<posy>0</posy>
			<width>270</width>
			<height>720</height>
			<texture border="109,109,128,128">option_menu_background.png</texture>
		</control>
		<control type="button">
			<posx>261</posx>
			<posy>329</posy>
			<width>34</width>
			<height>62</height>
			<texturenofocus>left_menu_mark.png</texturenofocus>
			<texturefocus>left_menu_mark.png</texturefocus>
			<onfocus>SetFocus(160)</onfocus>
			<onclick>SetFocus(160)</onclick>			
		</control>
		<control type="button">
			<posx>0</posx>
			<posy>0</posy>
			<width>270</width>
			<height>720</height>
			<texturenofocus>-</texturenofocus>
			<texturefocus>-</texturefocus>
			<onfocus>SetFocus(160)</onfocus>
			<onclick>SetFocus(160)</onclick>			
		</control>
	</include>
	<include name="BrowseLeftMenuSearchAnimation">
		<animation type="Conditional" condition="Window.IsActive(boxee_search.xml)">
			<effect type="fade" start="100" end="50" time="200"/>
			<effect type="slide" start="0,0" end="0,276" center="auto" time="400" tween="cubic" easing="inout"/>
		</animation>
	</include>
	<include name="BrowseLeftMenuAnimation">
		<animation effect="slide" start="0" end="-270" time="400" tween="cubic" easing="inout" condition="!ControlGroup(8000).HasFocus">Conditional</animation>
	</include>
