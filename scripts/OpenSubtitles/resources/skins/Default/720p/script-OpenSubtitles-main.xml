<window>
  <defaultcontrol>101</defaultcontrol>
  <coordinates>
    <system>1</system>
    <posx>0</posx>
    <posy>0</posy>
  </coordinates>
  <animation effect="fade" time="250">WindowOpen</animation>
  <animation effect="fade" time="250">WindowClose</animation>
  <controls>
    <control type="group">
      <posx>300</posx>
      <posy>50</posy>
      <control type="image">
        <posx>0</posx>
        <posy>0</posy>
        <width>690</width>
        <height>600</height>
        <texture border="40">default-panel.png</texture>
      </control>
      <!-- ** Required ** Do not change <id> or <type> -->
      <control type="label">
        <description>textarea</description>
        <posx>30</posx>
        <posy>10</posy>
        <width>630</width>
        <height>20</height>
        <font>font21</font>
        <textcolor>white</textcolor>
        <label>Powered by OpenSubtitles.org</label>
      </control>
      <control type="image">
        <posx>15</posx>
        <posy>50</posy>
        <width>670</width>
        <height>2</height>
        <texture>default-separatorOSD.png</texture>
      </control>
      <control type="label" id="100">
        <description>textarea</description>
        <posx>30</posx>
        <posy>60</posy>
        <width>630</width>
        <height>40</height>
        <font>font18</font>
        <textcolor>white</textcolor>
        <aligny>center</aligny>
        <wrapmultiline>true</wrapmultiline>
      </control>
      <control type="image">
        <posx>15</posx>
        <posy>110</posy>
        <width>670</width>
        <height>2</height>
        <texture>default-separatorOSD.png</texture>
      </control>
      <!-- ** Required ** Do not change <id> or <type> -->
      <control type="list" id="120">
        <posx>20</posx>
        <posy>126</posy>
        <width>640</width>
        <height>418</height>
        <onleft>120</onleft>
        <onright>120</onright>
        <onup>110</onup>
        <ondown>110</ondown>
        <viewtype label="535">list</viewtype>
        <pagecontrol>121</pagecontrol>
        <scrolltime>200</scrolltime>
        <itemlayout height="38">
          <control type="image">
            <posx>0</posx>
            <posy>36</posy>
            <width>640</width>
            <height>2</height>
            <texture border="10">default-list-nofocus.png</texture>
          </control>
          <control type="image">
            <posx>5</posx>
            <posy>13</posy>
            <width>18</width>
            <height>12</height>
            <info>Listitem.Thumb</info>
          </control>
          <control type="label">
            <posx>30</posx>
            <posy>0</posy>
            <width>100</width>
            <height>38</height>
            <font>font18</font>
            <aligny>center</aligny>
            <textcolor>white</textcolor>
            <selectedcolor>green</selectedcolor>
            <align>left</align>
            <info>ListItem.Label2</info>
          </control>
          <control type="label">
            <posx>150</posx>
            <posy>0</posy>
            <width>450</width>
            <height>38</height>
            <font>font18</font>
            <aligny>center</aligny>
            <textcolor>white</textcolor>
            <selectedcolor>green</selectedcolor>
            <align>left</align>
            <info>ListItem.Label</info>
          </control>
        </itemlayout>
        <focusedlayout height="38">
          <control type="image">
            <posx>0</posx>
            <posy>0</posy>
            <width>640</width>
            <height>38</height>
            <texture border="10">default-list-focus.png</texture>
            <visible>Control.HasFocus(120)</visible>
          </control>
          <control type="image">
            <posx>5</posx>
            <posy>13</posy>
            <width>18</width>
            <height>12</height>
            <info>Listitem.Thumb</info>
          </control>
          <control type="label">
            <posx>30</posx>
            <posy>0</posy>
            <width>100</width>
            <height>38</height>
            <font>font18</font>
            <aligny>center</aligny>
            <textcolor>white</textcolor>
            <selectedcolor>green</selectedcolor>
            <align>left</align>
            <info>ListItem.Label2</info>
          </control>
          <control type="label">
            <posx>150</posx>
            <posy>0</posy>
            <width>450</width>
            <height>38</height>
            <font>font18</font>
            <aligny>center</aligny>
            <textcolor>white</textcolor>
            <selectedcolor>green</selectedcolor>
            <align>left</align>
            <info>ListItem.Label</info>
          </control>
        </focusedlayout>
      </control>
      <control type="group">
        <posx>666</posx>
        <posy>126</posy>
        <control type="scrollbar" id="121">
          <description>scroll bar indicator for lists</description>
          <posx>0</posx>
          <posy>0</posy>
          <width>12</width>
          <height>418</height>
          <texturesliderbackground border="6">default-scroll_back.png</texturesliderbackground>
          <texturesliderbar border="6">default-scroll_mid.png</texturesliderbar>
          <texturesliderbarfocus border="6">default-scroll_mid.png</texturesliderbarfocus>
          <textureslidernib>-</textureslidernib>
          <textureslidernibfocus>-</textureslidernibfocus>
          <orientation>vertical</orientation>
          <showonepage>false</showonepage>
          <visible>Control.IsVisible(120)</visible>
        </control>
      </control>
      <control type="group" id="110">
        <control type="button" id="111">
          <posx>40</posx>
          <posy>530</posy>
          <width>120</width>
          <height>35</height>
          <label>Search</label>
          <font>font18</font>
          <onup>120</onup>
          <ondown>120</ondown>
          <onright>112</onright>
          <onleft>112</onleft>
          <texturefocus border="10">button-focus.png</texturefocus>
          <texturenofocus border="10">button-nofocus.png</texturenofocus>
        </control>
        <control type="button" id="112">
          <posx>540</posx>
          <posy>530</posy>
          <width>120</width>
          <height>35</height>
          <label>Settings</label>
          <font>font18</font>
          <onup>120</onup>
          <ondown>120</ondown>
          <onright>111</onright>
          <onleft>111</onleft>
          <texturefocus border="10">button-focus.png</texturefocus>
          <texturenofocus border="10">button-nofocus.png</texturenofocus>
        </control>
      </control>
    </control>
  </controls>
</window>
